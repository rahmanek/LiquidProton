{
  "name": "liquidproton",
  "version": "0.4.0",
  "description": "",
  "main": "public/index.html",
  "engine": "node 6.2.0",
  "scripts": {
    "start": "node index.js",
    "dev": "cd public && concurrently \"gulp dev --watch\" \"lite-server\"",
    "deploy": "gulp deploy",
    "view": "npm run deploy && cd builds/web && http-server -o",
    "lint": "eslint modules/*.js",
    "test": "gulp dev --watch",
    "postinstall": "npm run deploy && gulp bower && cd public/creative && npm install"
  },
  "author": "Ebad Rahman <rahmanek@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "jquery-ui": "^1.10.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "emcaVersion": "6",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "devDependencies": {
   "http-server": "^0.9.0",
   "lite-server": "^2.2.2"
  }
}
